FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      thermophysicalProperties;
}

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState perfectGas;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        nMoles          1;
        molWeight       28.97;
    }
    thermodynamics
    {
        Cp              1005;       // Specific heat capacity of air (J/kg/K)
        Hf              0;
    }
    transport
    {
        mu              1e-05;      // Dynamic viscosity (Pa.s)
        Pr              0.7;        // Prandtl number
    }
}


