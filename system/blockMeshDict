    (0, 11.5241, 0.8) // Vertex 11
    (0, 10.5241, 0.8) // Vertex 12
    (0, 10.5241, 0) // Vertex 13
    (8.251 1 2.0632) // Vertex 14
    (0 0 0.8) // Vertex 15
